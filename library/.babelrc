{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "debug": true,
        "useBuiltIns": "usage",
        "corejs": "3",
        "targets": {
          "browsers": ["last 2 versions", "iOS >= 9", "safari >= 10", "not dead"]
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties"
  ]
}